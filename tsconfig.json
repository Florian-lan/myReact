{
    "compileOnSave": true,
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "baseUrl": "./packages",
      "noImplicitReturns":false,
      "noUnusedParameters": false,
      "noUnusedLocals": false,
      "noEmit": true,
      "isolatedModules": true,
      "resolveJsonModule": true,
      

      "paths":{
        "hostConfig": ["./react-dom/src/hostConfig.ts"] 
      }
    },

  }
  